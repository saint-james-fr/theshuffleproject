    "@types/puppeteer": "^7.0.4",
    "axios": "^1.11.0",
    "cheerio": "^1.1.2",
    "csv-parser": "^3.2.0",
    "csv-writer": "^1.6.0",
    "disconnect": "^1.2.2",
    "puppeteer": "^24.17.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
